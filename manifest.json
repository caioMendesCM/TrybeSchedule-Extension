{
  "name": "Trybe Clock",
  "description": "gets agenda from slack",
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["alarms", "activeTab", "scripting", "storage"],
  "content_scripts": [
    {
      "matches": ["https://app.slack.com/*"],
      "js": ["content.js"]
    }
  ]
}