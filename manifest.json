{
  "name": "Trybe Clock",
  "description": "Some",
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://app.slack.com/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["activeTab", "scripting"]
}