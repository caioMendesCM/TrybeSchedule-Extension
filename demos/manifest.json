{
  "background": {"scripts": ["index.js"]},
  "manifest_version": 2,
  "name": "My Trybe Clock",
  "description": "Some",
  "version": "0.1",
  "app": {
    "urls": [
      "http://certification.googleapps.com/app-info/"
    ],
    "launch": {
      "web_url": "http://certification.googleapps.com/"
    }
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
   },
  "content_scripts": [
    {
    "matches": ["https://app.slack.com"],
    "js": ["index.js"]
    }
  ]
}